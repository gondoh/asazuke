{
    "name" : "mshiba/asazuke",
    "type" : "library",
    "license" : "MIT",
    "keywords" : [
        "api",
        "Pickles2"
    ],
    "homepage" : "http://pickles2.pxt.jp/tools/asazuke/",
    "authors" : [{
            "name" : "Misaki Shibata",
            "email" : "misaki.pink@gmail.com"
        }
    ],
    "minimum-stability" : "stable",
    "repositories": [],
    "require" : {
        "php" : ">=5.3.0",
        "zendframework/zend-servicemanager" : "2.5.1",
        "zendframework/zend-stdlib" : "2.5.1",
        "zendframework/zend-text" : "2.5.1",
        "symfony/finder" : "~2.7"
    },
    "require-dev" : {
        "phpunit/phpunit" : "4.1.*"
    },
    "config": {
        "process-timeout": 86400
    },
    "scripts":{
         "darwin-chmod": ["chmod -R 777 src/data", "chmod 777 src/Asazuke.log desktop.sql"],
         "win32-copy-conf": ["copyConf.bat"],
         "test" : ["vendor/phpunit/phpunit/phpunit --configuration phpunit.xml"]
    },
    "autoload" : {
        "psr-4" : {
            "Mshiba\\Px2lib\\Asazuke\\" : "src/"
        }
    }
}
